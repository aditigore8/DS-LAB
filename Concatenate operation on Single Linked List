#include<stdio.h>
#include<stdlib.h>
typedef struct Node* NODE;
struct Node{
	int data;
struct Node* next;
};
NODE addNode(NODE list,int data){
	NODE newNode = (NODE)malloc(sizeof(struct Node));
	newNode->data = data;
	newNode->next = NULL;
	if(!list){
		return newNode;
	}
	NODE temp = list;
	while(temp->next){
		temp = temp->next;
	}
	temp->next = newNode;
	return list;
}
void displayList(NODE list){
	for(NODE temp = list;temp;temp=temp->next){
		printf("%d --> ",temp->data);
	}
	printf("NULL\n");
}
NODE concatenate(NODE list1,NODE list2){
	if(!list1) return list2;
	NODE temp = list1;
	while(temp->next){
		temp= temp->next;
	}
	temp->next = list2;
	return list1;
}
